{
	"default": [
		"<div id=\"\">",
		"{{ slot }}",
		"</div>"
	],
	"index": [
		"<div id=\"\">",
		"\t<div id=\"\">",
		"{{ slot }}",
		"\t</div>",
		"</div>"
	],
	"header:fly": [
		"<div class=\"flex gap align:item x header:size fixed box-shadow\" component=\"header:fly\">",
		"\t<div class=\"padding:left\">",
		"\t\t<button class=\"button:icon font:large\">",
		"\t\t\t<div class=\"icon:container icon:margin\">",
		"\t\t\t\t<span class=\"icon:material\">menu</span>",
		"\t\t\t</div>",
		"\t\t</button>",
		"\t</div>",
		"\t<div class=\"flex align:item\">",
		"{{ theme:component header:logo }}",
		"\t</div>",
		"\t<div class=\"flex align:item justify:item flex:grow gap\">",
		"\t\t<div class=\"flex align:item gap\" computer>",
		"\t\t\t<div class=\"flex align:item\">",
		"{{ theme:component header button:movie }}",
		"{{ theme:component header button:tv }}",
		"{{ theme:component header button:people }}",
		"\t\t\t</div>",
		"\t\t</div>",
		"\t</div>",
		"\t<div class=\"flex align:item\">",
		"\t\t<input id=\"search\" class=\"padding padding-horizontal:medium no-border border:radius header-fly-form-search\" type=\"search\" placeholder=\"Search ...\">",
		"\t</div>",
		"\t<div class=\"flex align:item gap padding:right\">",
		"\t\t<button class=\"button:icon\">",
		"\t\t\t<div class=\"icon:container icon:margin font:large\">",
		"\t\t\t\t<span class=\"icon:material\">search</span>",
		"\t\t\t</div>",
		"\t\t</button>",
		"\t\t<button class=\"button:icon\" computer>",
		"\t\t\t<div class=\"icon:container icon:margin font:large\">",
		"\t\t\t\t<span class=\"icon:material\">notifications</span>",
		"\t\t\t</div>",
		"{{ component notification:float }}",
		"\t\t</button>",
		"\t\t<button class=\"button:icon\">",
		"\t\t\t<div class=\"icon:container icon:margin font:large\">",
		"\t\t\t\t<span class=\"icon:material\">account_circle</span>",
		"\t\t\t</div>",
		"{{ component account-manager:float }}",
		"\t\t</button>",
		"\t\t<button class=\"button:icon\">",
		"\t\t\t<div class=\"icon:container icon:margin font:large\">",
		"\t\t\t\t<span class=\"icon:material\">more_vert</span>",
		"\t\t\t</div>",
		"\t\t\t<!--section class=\"nav:size top:port right border-radius border-size background:color box-shadow\">",
		"\t\t\t\t<div class=\"flex flex:column x\">",
		"\t\t\t\t\t<div>asd</div>",
		"\t\t\t\t\t<div>asd</div>",
		"\t\t\t\t</div>",
		"\t\t\t</section-->",
		"\t\t</button>",
		"\t</div>",
		"</div>"
	],
	"logo:simple": [
		"<div class=\"flex align:item gap\" component=\"logo:simple\">",
		"\t<div><img src=\"{{ asset:image }}/logo.png\" height=\"32\"></div>",
		"\t<div class=\"flex flex:column\">",
		"\t\t<div class=\"font-family:logo font:medium font:bold text-gradient\">{{ site:name }}</div>",
		"\t</div>",
		"</div>"
	],
	"logo:sample": [
		"<a href=\"/\" class=\"flex align:item gap\" component=\"logo:sample\">",
		"\t<div><img src=\"{{ asset:image }}/logo.png\" height=\"32\"></div>",
		"\t<div class=\"flex flex:column\">",
		"\t\t<div class=\"font-family:logo font:intermediate font:bold text-gradient\">{{ site:name }}</div>",
		"\t\t<div class=\"font:small font:bold\" style=\"color: rgba(var(--color))\">{{ site:tagline }}</div>",
		"\t</div>",
		"</a>"
	],
	"footer:sensitive": [
		"<div component=\"footer:sensitive\">",
		"</div>"
	],
	"footer:info": [
		"<div class=\"flex flex:column align:item gap:tiny font:tiny\" component=\"footer:info\">",
		"\t<div>Copyright &copy; 2025 <a href=\"{{ base_url }}\">{{ site:name }}</a></div>",
		"\t<div>All Right's Reserved</div>",
		"\t<div>Logo's Trademark's on this Site are the Property of their Respective Owner's</div>",
		"\t<div>Website Engine Code & Design is Powered By <a href=\"mailto:{{ author:email }}\">{{ author:name }}</a></div>",
		"</div>"
	],
	"notification:float": [
		"<section class=\"flex:column nav-chrome:size top:port right text-align:left border:radius background:color box-shadow no-overflow transition:visibility\">",
		"\t<div class=\"flex align:item gap padding:vertical padding-horizontal:size\">",
		"\t\t<div><img src=\"{{ asset:image }}/logo.png\" height=\"16\"></div>",
		"\t\t<div class=\"font:bold\">Notification</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<div class=\"flex flex:column gap padding:vertical padding-horizontal:size\">",
		"\t\t<div class=\"font-bold:pop\">No Activity</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<div class=\"font-size:pop line-spacing:small padding:vertical padding-horizontal:size\">",
		"\t\tOur Information, Activity will appear here, such as when we add new Movie and/or TV Show.",
		"\t</div>",
		"</section>"
	],
	"account-manager:float": [
		"<section class=\"flex:column nav-chrome:size top:port right text-align:left border:radius background:color box-shadow no-overflow transition:visibility\">",
		"\t<div class=\"flex align:item gap padding:vertical padding-horizontal:size\">",
		"\t\t<div><img src=\"{{ asset:image }}/logo.png\" height=\"16\"></div>",
		"\t\t<div class=\"flex:grow font:bold\">Account Manager</div>",
		"\t\t<div class=\"color:gray-pop\">DISABLED</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<div class=\"flex align:item gap padding:vertical padding-horizontal:size\">",
		"\t\t<div class=\"icon:container icon:margin font:big text-gradient\">",
		"\t\t\t<span class=\"icon:material\">person_shield</span>",
		"\t\t</div>",
		"\t\t<div class=\"flex flex:column\">",
		"\t\t\t<div class=\"font-bold:pop\">Guest</div>",
		"\t\t\t<div class=\"color:gray-pop\">guest@{{ domain:name }}</div>",
		"\t\t</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<a href=\"/\" class=\"flex align:item gap padding:vertical padding-horizontal:size a-hover:mono-pop\">",
		"\t\t<div class=\"icon:container icon:margin font:big text-gradient\">",
		"\t\t\t<span class=\"icon:material\">forum</span>",
		"\t\t</div>",
		"\t\t<div class=\"flex flex:column\">",
		"\t\t\t<div class=\"font-bold:pop color\">Support</div>",
		"\t\t\t<div class=\"color:gray-pop\">support@{{ domain:name }}</div>",
		"\t\t</div>",
		"\t</a>",
		"\t{{ component separator:mono }}",
		"\t<div class=\"font-size:pop line-spacing:small padding:vertical padding-horizontal:size\">",
		"\t\tWe keep your <u>IP</u>, <u>Name</u>, <u>Email</u>, <u>Profile Picture</u> Safe.",
		"\t\t<br>",
		"\t\tFeel Free to Watch and Download from our Website.",
		"\t\t<br>",
		"\t\tJust follow our <a href=\"{{ base_url }}{{ router page privacy-policy }}\">Privacy Policy</a> and <a href=\"{{ base_url }}{{ router page term_of_use }}\">Term's of Use</a>.",
		"\t</div>",
		"</section>"
	],
	"button:inline": [
		"<button class=\"button:inline\">",
		"\t<div class=\"flex align:item gap:small padding-vertical:small padding:horizontal\">",
		"\t\t<span class=\"font-bold:pop\">{{ title }}</span>",
		"\t\t<div class=\"icon:container icon:margin font:intermediate\"><span class=\"icon:material\">arrow_drop_down</span></div>",
		"\t</div>",
		"\t<section class=\"flex:column nav:size top:port left border:radius background:color box-shadow no-overflow transition:visibility\">",
		"{{ slot }}",
		"\t</section>",
		"</button>"
	],
	"button:inline anchor:container": [
		"<div class=\"flex flex:column text-align:left\">",
		"{{ slot }}",
		"</div>"
	],
	"button:inline anchor-more:container": [
		"<div class=\"flex font:small text-align:left gap padding\" menu=\"sample:link-more\">",
		"\t<div class=\"flex flex:column flex:grow\">",
		"{{ left }}",
		"\t</div>",
		"\t<div class=\"flex flex:column flex:grow\">",
		"{{ right }}",
		"\t</div>",
		"</div>"
	],
	"button:inline anchor": [
		"<a href=\"{{ permalink }}\" class=\"flex align:item gap padding:li a:static\">",
		"\t<div class=\"icon:container icon:margin\"><span class=\"icon:material\">{{ icon }}</span></div>",
		"\t<span class=\"font-bold:pop flex:grow\">{{ title }}</span>",
		"\t<span class=\"font:small font-bold:pop\" style=\"color: rgba(var(--color),0.75);\">{{ quantity }}</span>",
		"</a>"
	],
	"button:inline anchor:more": [
		"<a href=\"{{ permalink }}\" class=\"flex align:item gap padding:small padding:horizontal a:static\">",
		"\t<span class=\"flex:grow\">{{ title }}</span>",
		"</a>"
	],
	"separator:mono": [
		"<div class=\"separator:mono\"></div>"
	],
	"youtube:embed": [
		"<iframe width=\"560\" height=\"315\" src=\"{{ url }}\" title=\"{{ title }}\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share\" referrerpolicy=\"strict-origin-when-cross-origin\" allowfullscreen></iframe>"
	],
	"icon:material more horizontal": [
		"<div class=\"icon:container icon:margin\"><span class=\"icon:material\">more_horiz</span></div>"
	],
	"icon:material sort by alpha": [
		"<div class=\"icon:container icon:margin\"><span class=\"icon:material\">sort_by_alpha</span></div>"
	]
}