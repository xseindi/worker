		"\t\t<div class=\"font-size:small\">DISABLED</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<div class=\"flex align:item gap padding:vertical padding-horizontal:intermediate\">",
		"\t\t<div class=\"icon:container icon:margin font:big text-gradient\">",
		"\t\t\t<span class=\"icon:material\">person_shield</span>",
		"\t\t</div>",
		"\t\t<div class=\"flex flex:column\">",
		"\t\t\t<div class=\"font-bold:pop color\">Guest</div>",
		"\t\t\t<div class=\"\" color=\"mono:chrome\">guest@{{ domain:name }}</div>",
		"\t\t</div>",
		"\t</div>",
		"\t{{ component separator:mono }}",
		"\t<a href=\"/\" class=\"flex align:item gap padding:vertical padding-horizontal:intermediate a-hover:mono-pop\">",
		"\t\t<div class=\"icon:container icon:margin font:big text-gradient\">",
		"\t\t\t<span class=\"icon:material\">forum</span>",
		"\t\t</div>",
		"\t\t<div class=\"flex flex:column\">",
		"\t\t\t<div class=\"font-bold:pop color\">Support</div>",
		"\t\t\t<div class=\"\" color=\"mono:chrome\">support@{{ domain:name }}</div>",
		"\t\t</div>",
		"\t</a>",
		"\t{{ component separator:mono }}",
